{
  "name": "dateformat-converter",
  "version": "0.0.0-semantic-released",
  "description": "Package to convert between dateformats",
  "main": "index.js",
  "scripts": {
    "setup": "npm install && npm run validate",
    "lint": "eslint .",
    "test": "cross-env NODE_ENV=test nyc mocha --opts ./mocha.opts \"src/**/*.test.js\"",
    "test:results": "npm run open -- coverage\\lcov-report\\index.html\"",
    "report-coverage": "codecov",
    "prebuild": "rimraf dist",
    "build": "cross-env NODE_ENV=production babel --quiet src -d dist",
    "validate": "run-s test lint build",
    "commit": "git-cz",
    "semantic-release": "semantic-release pre && npm publish && semantic-release post",
    "precommit": "npm run lint",
    "prepush": "npm run validate",
    "open": "open-cli",
    "commitmsg": "validate-commit-msg"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/tigermarques/dateformat-converter.git"
  },
  "keywords": [
    "date",
    "dateformat",
    "convert"
  ],
  "author": "JGM",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/tigermarques/dateformat-converter/issues"
  },
  "homepage": "https://github.com/tigermarques/dateformat-converter#readme",
  "devDependencies": {
    "babel-cli": "6.24.1",
    "babel-core": "6.25.0",
    "babel-eslint": "7.2.3",
    "babel-preset-es2015": "6.24.1",
    "chai": "4.1.0",
    "codecov": "2.3.0",
    "commitizen": "2.9.6",
    "cross-env": "5.0.3",
    "cz-conventional-changelog": "2.0.0",
    "eslint": "4.3.0",
    "eslint-plugin-import": "2.7.0",
    "eslint-plugin-mocha": "4.11.0",
    "husky": "0.14.3",
    "mocha": "3.5.0",
    "npm-run-all": "4.0.2",
    "nyc": "11.1.0",
    "open-cli": "1.0.5",
    "semantic-release": "6.3.6",
    "semantic-release-cli": "3.0.3",
    "validate-commit-msg": "2.14.0"
  },
  "config": {
    "commitizen": {
      "path": "node_modules/cz-conventional-changelog"
    }
  }
}
